      <ruleml:And>
        <ruleml:Atom>
          <ruleml:Rel>ground floor</ruleml:Rel>
        </ruleml:Atom>
      </ruleml:And>
    </ruleml:if>
    <ruleml:then>
      <lrml:Permitted>
        <ruleml:Negation>
          <ruleml:Atom>
            <ruleml:Rel>
