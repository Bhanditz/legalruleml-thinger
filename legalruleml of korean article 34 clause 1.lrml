    <ruleml:if>
      <ruleml:And>
        <ruleml:Atom>
          <ruleml:Rel></ruleml:Rel>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel></ruleml:Rel>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel></ruleml:Rel>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel>less than</ruleml:Rel>
          <ruleml:Var>distance living room</ruleml:Var>
          <ruleml:Data>40meters</ruleml:Data>
